{
  "name": "SimpleBox",
  "description": "Creates a simple box with specified dimensions",
  "category": "basic",
  "parameters": {
    "width": {
      "type": "number",
      "description": "Width of the box in mm"
    },
    "depth": {
      "type": "number", 
      "description": "Depth of the box in mm"
    },
    "height": {
      "type": "number",
      "description": "Height of the box in mm"
    }
  },
  "steps": [
    {
      "tool": "CreateSketch",
      "parameters": {
        "plane": "xy"
      }
    },
    {
      "tool": "DrawRectangle",
      "parameters": {
        "width": "{{width}}",
        "depth": "{{depth}}"
      }
    },
    {
      "tool": "Extrude",
      "parameters": {
        "height": "{{height}}"
      }
    }
  ]
}